#include <stdio.h>
int main(){
    int n,k,i,a[10];
    printf("Enter number of elements:");
    scanf("%d",&n);
    printf("Enter %d elements:",n);
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    printf("Enter number to search:");
    scanf("%d",&k);
    
    for(i=0;i<=n;i++){
        if(a[i]==k){
            printf("Element found at %d",i);
            break;
        }
    }
    if(i==n+1){
        printf("Element not found");
    }
}
